<ssase>

<vm id="sas">

           
           
           <adaptation cost_unit="0.01" /> <!-- next 0.05 seems to be good, retry? -->
           
           <service name="rubis_software">
           <softwareControlPrimitive>
  <item name="HAVE_CRYPTO" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="1" price_per_unit="0.5" switchoff="true" />
<item name="HAVE_HASH" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="1" price_per_unit="0.5" switchoff="true" />
<item name="HAVE_REPLICATION" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="1" price_per_unit="0.5" switchoff="true" />
<item name="HAVE_VERIFY" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="1" price_per_unit="0.5" switchoff="true" />
<item name="HAVE_SEQUENCE" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="1" price_per_unit="0.5" switchoff="true" />
<item name="HAVE_STATISTICS" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="1" price_per_unit="0.5" switchoff="true" />
<item name="DIAGNOSTIC" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="1" price_per_unit="0.5" switchoff="true" />
<item name="PAGESIZE" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="5" price_per_unit="0.5" switchoff="false" />
<item name="CACHESIZE" provision="0" constraint="-1" differences="1" pre_to_max="0.7" pre_of_max="0.1" min="0" max="4" price_per_unit="0.5" switchoff="false" />
       
           </softwareControlPrimitive>


         
           <QoS>
           <!-- This constraint="-1" will forcly trigger adaptation at each sampling interval -->
             <!-- Unit is sec -->
             <!-- constraint="5"  -->
            <item name="Throughput" constraint="-0.05" is_min="true" pre_to_change="0.7" sla_per_unit="3.5"  />
             <!-- constraint="6"  -->
            <item name="Cost" constraint="5000" is_min="true" pre_to_change="0.7" sla_per_unit="0.5"  />
             <item name="Latency" constraint="-0.05" is_min="true" pre_to_change="0.7" sla_per_unit="0.5"  />
          
           </QoS>
           
           
        </service>
</vm>


</ssase>

